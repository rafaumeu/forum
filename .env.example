# Database Configuration
# Formato: postgresql://USER:PASSWORD@HOST:PORT/DATABASE_NAME?schema=public
DATABASE_URL=postgresql://USER:PASSWORD@HOST:PORT/DATABASE_NAME?schema=public

# JWT Authentication Keys
# Gere chaves seguras usando: openssl rand -base64 32
JWT_SECRET_PRIVATE_KEY=GENERATE_A_SECURE_PRIVATE_KEY_HERE
JWT_SECRET_PUBLIC_KEY=GENERATE_A_SECURE_PUBLIC_KEY_HERE

# Cloudflare Configuration
CLOUDFLARE_ACCOUNT_ID=YOUR_CLOUDFLARE_ACCOUNT_ID

# AWS S3 Storage Configuration
AWS_BUCKET_NAME=YOUR_BUCKET_NAME
AWS_ACCESS_KEY_ID=YOUR_AWS_ACCESS_KEY
AWS_SECRET_ACCESS_KEY=YOUR_AWS_SECRET_KEY
